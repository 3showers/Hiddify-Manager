{
	"inbounds": [
		{
			"tag":"vless-tcp-v1",
			"listen": "127.0.0.1",
			"listen_port":2021,
			"tcp_fast_open": true,
			"sniff": true,
			"sniff_override_destination": true,
			"domain_strategy": "prefer_ipv4",
			"proxy_protocol":true,	
			"type": "vless",
			"users": [
    		    {% for u in users %}
    				{"name": "{{ u['uuid'] }}@hiddify.com", "uuid": "{{ u['uuid'] }}"},
				{% endfor%}
  			],
			"transport": {
				"type": "http",
				"path":"",
				"method":"",
				//"path": "/{{ hconfigs['path_vless'] }}{{ hconfigs['path_tcp'] }}",
				"idle_timeout": "15s",
				"ping_timeout": "15s"
			},
			{%if hconfigs['mux_enable']%}
				{% include "includes/singbox_multiplex.json.j2"%}
			{%endif%}
		}	  
	]
}
